{
    "manifest_version": 3,
    "name": "Data Grabber",
    "version": "1.0",
    "key": "",
    "permissions": ["activeTab","scripting","identity"],
    "oauth2": {
      "client_id": "726590587489-opvsm92l9au2i076keuj3mrq83mkr6qg.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
    },
    "background": {
      "service_worker": "./popup/background.js"
    },
    "host_permissions": ["http://localhost:5000/*"],
    "action": {
      "default_popup": "./popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["./popup/background.js"]
      }
    ]
}